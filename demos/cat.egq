#!./evilcandy

let Io = import("io.evc", "x");

// Print myself
(function(name) {
    try {
        let f = Io.open(name, "r");
        let s;
        while (true) {
            s = f.read();
            if (s == null)
                break;
            // currently print always adds a newline
            print(s.rstrip("\n"));
        }
    } catch (e) {
        print('Got a', e[0], ':', e[1]);
    }
})("demos/cat.egq");

