let cat = function(name) {
        let f = Io.open(name, "r");
        if (typeof(f) == "string") {
                print("Cannot open '{}', reason='{}'".format(name, f));
                exit();
        }
        while (!f.eof()) {
                print(f.readstr());
        }
};

cat("demos/cat.egq");

