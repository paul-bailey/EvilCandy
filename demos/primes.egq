#!./evilcandy

// basically a test of for-else and break
let primes_to = function(bound) {
        for (n, range(2, bound)) {
                for (x, range(2, n)) {
                        if ((n % x) == 0)
                                break;
                } else {
                        print(n, end=' ');
                }
        }
};

primes_to(20);
print('');

