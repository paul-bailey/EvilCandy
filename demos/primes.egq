// basically a test of for-else and break
let primes_to = function(bound) {
        for (let n = 2; n < bound; n++) {
                for (let x = 2; x < n; x++) {
                        if ((n % x) == 0)
                                break;
                } else {
                        print("{}".format(n));
                }
        }
};

setnl(" ");
primes_to(20);
setnl("\n");
print("");

